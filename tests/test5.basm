.main:
    mov ax 10    //; loop stop
    push 1       //; loop variable
    push 10 
                 //; loop start
    push 10
    add
    push [1]     //;+ add 1 to loop var
    push 1
    add             
    mov [1] [sp] //;+ store loop var
    push ax      //;+ compare loop var to 10
    eq           //;+ once loop var hit 10 it will get out of the loop
    jz 1009    
                 //; loop end
    end          //; tos: 100